{
  "name": "@jupyterlite/xeus-kernel",
  "version": "0.1.0-alpha.6",
  "description": "JupyterLite - xeus Kernel",
  "homepage": "https://github.com/jupyterlite/jupyterlite",
  "bugs": {
    "url": "https://github.com/jupyterlite/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlite/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js",
    "src/xeus_dummy.wasm"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "lib/*.wasm",
    "src/*.wasm",
    "style/*.css",
    "style/index.js",
    "src/xeus_dummy.worker.js",
    "src/xeus_dummy.js",
    "src/xeus_dummy.wasm",
    "py/**/*.whl"
  ],
  "scripts": {
    "build:dockerimage": "docker build -t mydockerimage  --build-arg USER_ID=$(id -u) --build-arg GROUP_ID=$(id -g) .",
    "build:emscripten": "echo $(pwd) && docker run --rm -v $(pwd):/src    -u $(id -u):$(id -g)  mydockerimage    ./build.sh",
    "copy-files1": "copyfiles -u 1 src/xeus_dummy.wasm  src/xeus_dummy.worker.js src/xeus_dummy.js lib",
    "copy-files2": "copyfiles -u 1 src/xeus_lua.wasm  src/xeus_lua.worker.js src/xeus_lua.js lib",
    "copy-files3": "copyfiles -u 1 src/xeus_lua_kernel.wasm  src/xeus_lua_kernel.worker.js src/xeus_lua_kernel.js lib",
    "build": "yarn run build:lib",
    "build:lib": "yarn run build:dockerimage && yarn run build:emscripten && yarn run copy-files1 && yarn run copy-files2 && yarn run copy-files3 && tsc -b",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "yarn run clean:lib && yarn run clean:python",
    "clean:lib": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "clean:python": "rimraf py/dist",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "mytest": "TS_NODE_PROJECT='./tsconfig.test.json' node  --experimental-wasm-threads --experimental-wasm-bulk-memory ./node_modules/.bin/_mocha  -r ts-node/register test/**/*.spec.ts",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlite/kernel": "^0.1.0-alpha.6"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@jupyterlab/testutils": "^3.1.0-rc.2",
    "@types/chai": "^4.2.21",
    "@types/emscripten": "1.39.5",
    "@types/jest": "^26.0.10",
    "@types/mocha": "^9.0.0",
    "babel-preset-minify": "0.5.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "jest": "^26.4.2",
    "mocha": "^9.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^10.2.0",
    "typescript": "~4.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "xeus": {
    "packages": {
      "py/xeus": "0.1.0a6",
      "py/ipykernel": "5.5.5",
      "py/nbformat": "4.2.0",
      "py/widgetsnbextension": "3.5.0"
    }
  },
  "styleModule": "style/index.js"
}
